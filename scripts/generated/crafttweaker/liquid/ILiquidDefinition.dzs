import crafttweaker.block.IBlock;
import crafttweaker.liquid.ILiquidStack;

zenClass ILiquidDefinition {
    val name as string;
    val displayName as string;
    val block as IBlock;
    var luminosity as int;
    var viscosity as int;
    var density as int;
    var temperature as int;
    var gaseous as bool;
    
    operator *(millibuckets as int) as ILiquidStack;
}