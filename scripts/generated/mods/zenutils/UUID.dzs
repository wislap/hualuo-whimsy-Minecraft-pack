
zenClass UUID {
    
    function getLeastSignificantBits() as long;
    
    function getMostSignificantBits() as long;
    
    static function randomUUID() as UUID;
    
    static function fromString(name as string) as UUID;
    
    function asString() as string;
    
    static function fromSignificantBits(most as long, least as long) as UUID;
    
    operator ==(o as any) as bool;
    
    operator compare(other as UUID) as int;
    
    operator as() as string;
}